{
  "name": "mcp-weather-server",
  "version": "1.0.0",
  "description": "MCP server for OpenWeather API integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-weather-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "dev:watch": "nodemon --watch src --ext ts --exec \"npm run dev\""
  },
  "keywords": [
    "mcp",
    "weather",
    "openweather"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.11",
    "typescript": "^5.7.2"
  }
}
